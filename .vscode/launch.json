    "version": "0.2.0",
    "configurations": [

        {
            "name": "(cat) Launch",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/cat",
            "args": ["/rohit/wrFile.txt"],
            "stopAtEntry": true,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        },
        {
            "name": "(rm) Launch",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/rm",
            "args": ["/abc/newFile.txt"],
            "stopAtEntry": true,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        },
        {
            "name": "(LN) Launch",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/ln",
            "args": ["/rohit/file.txt", "/rohit/nf.txt"],
            "stopAtEntry": true,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        }
    ]
}